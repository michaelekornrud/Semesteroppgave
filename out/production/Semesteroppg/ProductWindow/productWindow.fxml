<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="304.0" prefWidth="242.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProductWindow.Controller_ProductWindow">
<children>
    <TextField fx:id="txtProductName" layoutX="41.0" layoutY="106.0" promptText="Navn">
         <tooltip>
            <Tooltip text="Skriv inn navn på komponenttypen" />
         </tooltip></TextField>
    <TextField fx:id="txtNumberOfProducts" layoutX="41.0" layoutY="139.0" promptText="Antall">
         <tooltip>
            <Tooltip text="Skriv inn antall av gitt komponent" />
         </tooltip></TextField>
    <TextField fx:id="txtBrand" layoutX="41.0" layoutY="176.0" promptText="Merke">
         <tooltip>
            <Tooltip text="Skriv inn komponentmerke" />
         </tooltip></TextField>
    <TextField fx:id="txtPrice" layoutX="41.0" layoutY="209.0" promptText="Pris">
         <tooltip>
            <Tooltip text="Skriv inn prisen på komponenten" />
         </tooltip></TextField>
    <Button layoutX="44.0" layoutY="260.0" mnemonicParsing="false" onAction="#btnAdd" text="Legg til">
         <tooltip>
            <Tooltip text="Legg til komponent i lager" />
         </tooltip></Button>
      <Button fx:id="btnTibake" layoutX="141.0" layoutY="260.0" mnemonicParsing="false" onAction="#back" text="Tilbake">
         <tooltip>
            <Tooltip text="Gå tilbake til admin-vinduet" />
         </tooltip></Button>
      <ChoiceBox fx:id="choiceType" layoutX="44.0" layoutY="68.0" prefHeight="27.0" prefWidth="155.0">
         <tooltip>
            <Tooltip text="Komponenttype" />
         </tooltip></ChoiceBox>
</children>
</AnchorPane>
